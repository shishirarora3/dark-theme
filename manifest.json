{
  "name": "dark-theme",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome Extension For Converting complete web experience into dark theme",
  "homepage_url": "https://www.walmart.com",
  "content_scripts": [
    {
    "matches": ["<all_urls>"],
    "js": [
    ],
    "css": ["dark.css"]
    }
  ],
  "browser_action": {
    "default_title": "dark theme"
  },
  "permissions": [
    "activeTab",
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
